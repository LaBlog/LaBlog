{
    "name": "lablog/lablog",
    "description": "A simple but powerful blogging platform built on Laravel 4.",
    "authors": [
        {
            "name": "Joseph Ralph",
            "email": "joe@joescode.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "michelf/php-markdown": "1.4.0",
        "rcrowe/twigbridge": "0.5.*",
        "twig/extensions": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "psr-0": {
            "Lablog\\Lablog\\": "src/",
            "Lablog\\Lablog\\Controllers": "src/controllers"
        }
    },
    "minimum-stability": "dev"
}